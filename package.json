  },
  "keywords": [],
  "repository": "https://github.com/louiss0/template-tsup",
  "author": {
    "name": "Shelton Louis",
    "url": "https://github.com/louiss0"
  },
  "devDependencies": {
    "@anatine/zod-mock": "^3.13.3",
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@vitest/coverage-v8": "^0.34.6",
    "semantic-release": "^22.0.8",
    "tsup": "^8.0.1",
    "typescript": "^5.3.2",
    "vitest": "^0.34.6",
    "zod": "^3.22.4"
  }
}